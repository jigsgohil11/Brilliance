    <script>
        var url = window.location.href;
        var LastChar = url[url.length - 1];
        if (LastChar == "/") {
            url = url.slice(0, -1);
        }
        $(".page-sidebar-menu a").each(function () {
            if (url == (this.href)) {
                $(this).closest("li").addClass("active");
                $(this).closest("li").parent().parent().addClass("active");
            }
